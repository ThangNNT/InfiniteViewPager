<?xml version="1.0" encoding="utf-8"?>
<resources >
    <declare-styleable name="Indicator" >
        <attr name="nnt_viewPager" format="reference" />
        <attr name="nnt_activeColor" format="color" />
        <attr name="nnt_inactiveColor" format="color" />
        <attr name="nnt_indicatorHeight" format="dimension" />
        <attr name="nnt_activeWidth" format="dimension" />
        <attr name="nnt_inactiveWidth" format="dimension" />
        <attr name="nnt_spacing" format="dimension" />
        <attr name="nnt_corner_radiusX" format="dimension" />
        <attr name="nnt_corner_radiusY" format="dimension" />
        <attr name="nnt_activeOpacity" format="float" />
        <attr name="nnt_inactiveOpacity" format="float" />
        <attr name="nnt_animation_type" format="enum">
            <enum name="none" value="0"/>
            <enum name="worm" value="1"/>
            <enum name="thin_worm" value="2"/>
            <enum name="slide" value="3"/>
            <enum name="swap" value="4"/>
            <enum name="scale" value="5"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="InfiniteViewPager">
        <attr name="nnt_autoScroll" format="boolean"/>
        <attr name="nnt_delay_millis" format="integer" />
    </declare-styleable>
</resources>